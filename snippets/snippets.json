{
	"Block Comment":{
		"prefix":"/*",
		"body":"/*\n$0\n*/",
		"description":"Open a block comment"
	},
	"Class":{
		"prefix":"cls",
		"body":"class ${1:name} {\n\t$0\n}",
		"description":"Declare a class"
	},
	"Function":{
		"prefix":"func",
		"body":"function ${3:void} ${1:name}(${2:params}) {\n\t$0\n}",
		"description":"Declare a function"
	},
	"Print":{
		"prefix":["pr","sout","syso"],
		"body":"print($1)$0",
		"description":"Print a message to the console"
	}
}